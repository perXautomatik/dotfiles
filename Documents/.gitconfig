[core]
	autocrlf = false
[branch]
	autosetuprebase = always
[gc]
	autoDetach = false
[pull]
	rebase = true
[commit]
	gpgSign = false
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[user]
	email = you@example.com
	name = Your Name
