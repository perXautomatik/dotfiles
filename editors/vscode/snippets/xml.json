{
	"csproj": {
		"prefix": "csproj",
		"body": [
			"<Project Sdk=\"Microsoft.NET.Sdk\">",
			"",
			"\t<PropertyGroup>",
			"\t\t<OutputType>${1|Library,Exe,WinExe,Module|}</OutputType>",
